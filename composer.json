{
    "name": "antalaron/doctrine-twig-bundle",
    "type": "symfony-bundle",
    "description": "Twig Doctrine Bundle",
    "keywords": ["twig", "doctrine"],
    "homepage": "https://github.com/antalaron/doctrine-twig-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Antal Ãron",
            "email": "antalaron@antalaron.hu",
            "homepage": "http://antalaron.hu"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "~2.7|~3.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "<6.0.0",
        "friendsofphp/php-cs-fixer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Antalaron\\DoctrineTwigBundle\\": "src/Antalaron/DoctrineTwigBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Antalaron\\DoctrineTwigBundle\\Tests\\": "tests/Antalaron/DoctrineTwigBundle/Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
